function () {

    var timelines = {{{timelines}}},
        animTargets = {{{animTargets}}},
        rootTl = TimelineMax();


    timelines.forEach(function (timeline) {

        var prevTime = 0;

        var tl = TimelineMax();
        rootTl.add(tl);
        
        timeline.forEach(function (key, idx) {

            tl.to(animTargets, key.duration, key.options);
        });
    });

    return rootTl;
}